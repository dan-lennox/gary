{
  "name": "gary",
  "version": "1.0.0-alpha1",
  "description": "To do list bot that focuses on your top two priorities only.",
  "main": "bin/app.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node ./src/app.js",
    "server": "nodemon --inspect ./src/app.js",
    "client": "npm run start --prefix ui/server/client",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ui/server/client && npm run build --prefix ui/server/client"
  },
  "author": "Daniel Lennox",
  "license": "ISC",
  "dependencies": {
    "azure-storage": "^2.8.3",
    "botbuilder": "^3.15.0",
    "botbuilder-azure": "^3.1.0",
    "botbuilder-facebookextension": "^1.2.4",
    "concurrently": "^3.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "lodash": "^4.17.10",
    "moment": "^2.21.0",
    "node-cron": "^1.2.1",
    "nodemon": "^1.12.5",
    "path": "^0.12.7",
    "rxjs": "^5.5.4",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^3.8.1"
  }
}
